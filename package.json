{
  "name": "breakpoint-changes-rx",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jenssimon/breakpoint-changes-rx"
  },
  "author": "Jens Simon <https://github.com/jenssimon>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "breakpoints",
    "breakpoints-change",
    "viewport",
    "responsive",
    "rxjs",
    "observable"
  ],
  "type": "module",
  "main": "dist/breakpoints.js",
  "types": "dist/breakpoints.d.ts",
  "files": [
    "dist/breakpoints.js",
    "dist/breakpoints.d.ts",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint --fix --ext .tsx,.ts,.jsx,.js ./ && tsc --noEmit",
    "build": "tsc",
    "prepublishOnly": "tsc",
    "test": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "@jenssimon/eslint-config-base": "5.3.0",
    "@jenssimon/eslint-config-typescript": "3.3.3",
    "@types/jest": "27.4.0",
    "coveralls": "3.1.1",
    "eslint": "8.9.0",
    "jest": "27.5.1",
    "rxjs": "7.4.0",
    "ts-jest": "27.0.7",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "rxjs": ">=6"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "@jenssimon/base",
      "@jenssimon/typescript"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
