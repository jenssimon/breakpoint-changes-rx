{
  "name": "breakpoint-changes-rx",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jenssimon/breakpoint-changes-rx"
  },
  "author": "Jens Simon <https://github.com/jenssimon>",
  "license": "MIT",
  "keywords": [
    "breakpoints",
    "breakpoints-change",
    "viewport",
    "responsive",
    "rxjs",
    "observable"
  ],
  "type": "module",
  "main": "dist/breakpoints.js",
  "types": "dist/breakpoints.d.ts",
  "files": [
    "dist/breakpoints.js",
    "dist/breakpoints.d.ts",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint --fix --ext .tsx,.ts,.jsx,.js ./ && tsc --noEmit",
    "build": "tsc",
    "prepublishOnly": "tsc",
    "test": "jest --coverage",
    "ci": "run-s \"ci:*\"",
    "ci:lint": "eslint --ext .tsx,.ts,.jsx,.js ./",
    "ci:tsc": "tsc --noEmit",
    "ci:test": "jest --coverage"
  },
  "devDependencies": {
    "@jenssimon/eslint-config-base": "5.4.1",
    "@jenssimon/eslint-config-typescript": "3.4.2",
    "@types/jest": "27.5.1",
    "eslint": "8.15.0",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "npm-run-all": "4.1.5",
    "rxjs": "7.5.5",
    "ts-jest": "28.0.2",
    "typescript": "4.6.4"
  },
  "peerDependencies": {
    "rxjs": ">=6"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "@jenssimon/base",
      "@jenssimon/typescript"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "packageManager": "yarn@3.2.1"
}
